{
  "name": "symfony-notes/object-transformer-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Object Transformer Bundle",
  "keywords": ["symfony", "bundle", "object transformer", "json", "xml"],
  "homepage": "https://github.com/symfony-notes/object-transformer-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Zhuravel",
      "email": "zhooravell@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SymfonyNotes\\ObjectTransformerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": "^7.3.0",
    "symfony/framework-bundle": "4.2.*",
    "sensio/framework-extra-bundle": "^5.2",
    "symfony/serializer": "4.2.*",
    "symfony/validator": "4.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
